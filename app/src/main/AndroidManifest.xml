<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medtub2">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="MedTub"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MedTub2">
        <activity android:name=".AddMedecinActivity"
            android:parentActivityName=".contac2"></activity>
        <activity android:name=".contac2" />
        <activity android:name=".exam2" />
        <activity android:name=".log2" />
        <activity android:name=".sign2" />
        <activity android:name=".stat" />
        <activity android:name=".gps" />
        <activity android:name=".son" />
        <activity android:name=".camera" />
        <activity android:name=".sms" />
        <activity android:name=".form" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>